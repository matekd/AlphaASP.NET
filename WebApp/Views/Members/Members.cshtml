@model IEnumerable<Member>

@{
    ViewData["Title"] = "Members";
}
@await Html.PartialAsync("Partials/Components/_AddMember", new MemberModel())

<div class="main-header">
    <h2>@ViewData["Title"]</h2>
    <button class="btn blue add" data-modal="true" data-target="#addMemberModal">Add Member</button>
</div>

<div class="item-list wrap-list">
    @if(Model != null)
        foreach(var member in Model)
        {
            @await Html.PartialAsync("Partials/Components/_Member", member)
        }
</div>